<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="startAngle" format="integer" />
    <attr name="sweepAngle" format="integer" />
    <attr name="destinationSweepAngle" format="integer" />
    <attr name="useAnimation" format="boolean" />
    <attr name="autoAnimation" format="boolean" />
    <attr name="strokeWidth" format="dimension" />

    <declare-styleable name="GradientArcView">
        <attr name="startAngle" />
        <attr name="sweepAngle" />
        <attr name="destinationSweepAngle" />
        <attr name="useAnimation" />
        <attr name="autoAnimation" />
        <attr name="strokeWidth" />
        <attr name="gradientColors" format="reference" />
        <attr name="gradientColorPositions" format="reference" />
    </declare-styleable>

    <declare-styleable name="ArcView">
        <attr name="startAngle" />
        <attr name="sweepAngle" />
        <attr name="destinationSweepAngle" />
        <attr name="useAnimation" />
        <attr name="autoAnimation" />
        <attr name="strokeWidth" />
        <attr name="arcColor" format="color|reference" />
    </declare-styleable>
</resources>